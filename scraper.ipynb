    "def chunks(l, n):\n",
    "    \"\"\"Yield successive n-sized chunks from l.\"\"\"\n",
    "    for i in range(0, len(l), n):\n",
    "        yield l[i:i + n]\n",
    "\n",
    "Comments_chunk_all=list(chunks(results[0:545], 3))\n",
    "\n",
    "#*********************************************#\n",
    "#            THIS IS THE SCRAPER:             #\n",
    "#*********************************************#\n",
    "\n",
    "final_dataframe_list=[]\n",
    "\n",
    "for chunk in Comments_chunk_all:\n",
    "    \n",
    "    list_chunk_df=[]\n",
    "    \n",
    "    for item in chunk:\n",
    "        list_df = []\n",
    "\n",
    "        url_can = item\n",
    "        Session = requests.Session()\n",
    "        response = Session.get(url_can)\n",
    "        soup = BeautifulSoup(response.text,\"lxml\")\n",
    "\n",
    "        #First we ensure that we are scraping from the first page of reviews:\n",
    "        html = str(soup.find_all(attrs={\"class\":\"pageNum first taLnk \"}))\n",
    "\n",
    "     \n",
    "        try:\n",
    "            \n",
    "             #first we test if we are on the first page\n",
    "            if html == \"[]\": #\n",
    "                html = str(soup.find_all(attrs={\"class\":\"nav next taLnk \"}))\n",
    "            #if we are on the first page, we scrape and then always navigate to the next page\n",
    "                while html != \"[]\":\n",
    "                    info = information(url_can)[0] \n",
    "\n",
    "                    path = html.split('\"',6)[6].split('\"',2)[1]\n",
    "                    url_can = \"https://www.tripadvisor.com\" + path\n",
    "\n",
    "                    Session = requests.Session()\n",
    "                    response = Session.get(url_can)\n",
    "                    soup = BeautifulSoup(response.text,\"lxml\")\n",
    "\n",
    "                    html = str(soup.find_all(attrs={\"class\":\"nav next taLnk \"}))\n",
    "                    list_df.append(info)\n",
    "\n",
    "        #If we hit the last page(end of while loop) we use information_lastpage to retrive the info\n",
    "                else:\n",
    "                    info_last = information_lastpage(url_can, remaining_reviews) \n",
    "                    print(\"done\")\n",
    "            #if we are not on the first page, we navigate to the first page first\n",
    "            else:\n",
    "                path = html.split('\"',6)[6].split('\"',2)[1]\n",
    "                url_can = \"https://www.tripadvisor.com\" + path\n",
    "\n",
    "                html = str(soup.find_all(attrs = {\"class\":\"nav next taLnk \"}))\n",
    "\n",
    "                while html != \"[]\":\n",
    "                    info = information(url_can)[0]\n",
    "\n",
    "                    path = html.split('\"',6)[6].split('\"',2)[1]\n",
    "                    url_can = \"https://www.tripadvisor.com\" + path\n",
    "\n",
    "                    Session = requests.Session()\n",
    "                    response = Session.get(url_can)\n",
    "                    soup = BeautifulSoup(response.text,\"lxml\")\n",
    "                    html = str(soup.find_all(attrs={\"class\":\"nav next taLnk \"}))\n",
    "                    list_df.append(info)\n",
    "\n",
    "                else:\n",
    "                    info_last = information_lastpage(url_can, remaining_reviews) \n",
    "                    print(\"done\")\n",
    "\n",
    "            df = pd.concat(list_df).append(info_last)\n",
    "            df = df.set_index(\"date_of_review\")\n",
    "            list_chunk_df.append(df)\n",
    "            df2=pd.concat(list_chunk_df)\n",
    "        \n",
    "        except UnboundLocalError:\n",
    "            continue    \n",
    "    final_dataframe_list.append(df2)\n",
    "\n",
    "df_total = pd.concat(final_dataframe_list)\n",
    "df_total.to_csv(\"Name_File.csv\", header = True)"
   ]
