    "rome = rome[[\"city\", \"reviews\"]]\n",
    "\n",
    "all_comments = paris.append(london1).append(london2).append(brussels).append(barcelona).append(rome)\n",
    "all_comments = all_comments.sample(frac = 0.60).reset_index()\n",
    "all_comments['country_id'] = all_comments['city'].factorize()[0]\n",
    "#We factorize the countries by giving each country an index.  "
