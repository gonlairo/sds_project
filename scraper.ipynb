   "source": [
    "### Retrieve links for restaurants:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Note: The code below in an example of how to generate links for Paris restaurants specifically. \n",
    "#Below is the CSV-file in which the linkes retrieved from TripAdvisor will be placed in. For now it is empty:\n",
    "csvfile=\"Restaurant_Links.csv\"\n",
    "\n",
    "#Step 1: all overview pages for restaurants in Paris \n",
    "overview_urls=set()\n",
    "links = set() #this is where the first page info such as prices are in\n",
    "a=range(0,480) #This specifies the range of pages on TripAdvisor restaurants from which we are interested in retrieving links. \n",
    "random.seed(101)\n",
    "list1=random.sample(a, 40)\n",
    "for i in list1: #set your own limits - one range is approx. 30 restaurants\n",
    "    overview_urls.add(\"https://www.tripadvisor.com/Restaurants-g187147-oa\"+str(30*i)+\"-Paris_Ile_de_France.html#EATERY_LIST_CONTENTS\") \n",
    "    \n",
    "for overview in overview_urls:\n",
    "    url = overview\n",
    "    response = requests.get(url)\n",
    "\n",
    "    if response.ok:\n",
    "        html = response.text\n",
    "    else:\n",
    "        print('error')\n",
    "#Step 2: Get front page links for all restaurants in Paris\n",
    "\n",
    "    for link_loc in html.split('href=\"')[1:]:\n",
    "        link = link_loc.split('\"')[0]\n",
    "\n",
    "        if ('/Restaurant_Review' and \"#REVIEWS\") in link:\n",
    "            links.add('https://www.tripadvisor.com'+link)\n",
    "\n",
    "#Step 3: Use the front page links to get to the comment section - create links \n",
    "\n",
    "Comment_link=set()\n",
    "for res in links:\n",
    "    url=res\n",
    "    response = requests.get(url)\n",
    "    html= response.text\n",
    "    soup=BeautifulSoup(response.text, \"lxml\")\n",
    "    UserComments=[]\n",
    "    \n",
    "    #first find total review number\n",
    "    total_num = soup.find_all(attrs = {\"class\": \"reviews_header_count block_title\"})\n",
    "    \n",
    "    total_reviews_num = str(total_num).replace(\",\", '') # In France they neither seperate thousands by pause nor comma, which we correct for. \n",
    "    \n",
    "    total_reviews_num = int(total_reviews_num.split('(')[1].split(\")\")[0])\n",
    "    \n",
    "    if total_reviews_num>=50:\n",
    "    #find comment page for all restaurants with at least 50\n",
    "        for link_loc in html.split('href=\"')[1:]:\n",
    "                link = link_loc.split('\"')[0]\n",
    "                if (\"ShowUser\") in link:\n",
    "                    UserComments.append('https://www.tripadvisor.com'+link)\n",
    "        Comment_link.add(UserComments[0])\n",
    "    #don't look for the ones with too few\n",
    "    else:\n",
    "        continue #should skip it \n",
    "Comment_link_list=list(Comment_link)\n",
    "#Comment_link --> This is the set with a comment link (here we run the while loop that takes us to the first page)\n",
    "with open(csvfile, \"w\") as output:\n",
    "    writer = csv.writer(output, lineterminator='\\n')\n",
    "    for val in Comment_link_list:\n",
    "        writer.writerow([val]) \n"
   ]
  },
