   "source": [
    "### Preparation for the scraper:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "#At first we retrieve the CSV file containing the links of our restaurants in Paris which we retrieved in the last section:\n",
    "csvfile=\"Restaurant_Links.csv\"\n",
    "results = []\n",
    "with open(csvfile, newline='') as inputfile:\n",
    "    for row in csv.reader(inputfile):\n",
    "        results.append(row[0])\n",
    "        \n",
    "def information(url):\n",
    "    \"\"\" This function gets the name, the address and the total number of reviews of the selected restaurant(url). \n",
    "    Moreover, it also give us the date, the headline and the content(text) of each review in the url \"\"\"\n",
    "\n",
    "    response = requests.get(url) # We use the requests module to access the content of the webpage.\n",
    "    soup = BeautifulSoup(response.text, \"lxml\") # We use BeautifulSoup to work with HTML and be able to search the content.\n",
    "    \n",
    "    # We then obtain different information depending on the type of the class\n",
    "    name = soup.find_all(attrs ={\"class\": \"HEADING\"})\n",
    "    address = soup.find_all(attrs ={\"class\": \"format_address\"})\n",
    "    total = soup.find_all(attrs = {\"class\": \"reviews_header_count block_title\"})\n",
    "    date = soup.find_all(attrs = {\"class\":\"ratingDate relativeDate\"})\n",
    "    headline = soup.find_all(attrs={\"class\":\"noQuotes\"})\n",
    "    text = soup.find_all(attrs = {\"class\": \"partial_entry\"})\n",
    "    link_last = soup.find_all(attrs = {\"class\":\"pageNum last taLnk \"})\n",
    "\n",
    "    #In order to store the information we create three empty lists:\n",
    "    dates = []\n",
    "    headlines = []\n",
    "    reviews = []\n",
    "    \n",
    "    # 'loopcount' is the number of comments per page except the last one.\n",
    "    loopcount = 5\n",
    "    \n",
    "    #Below we split the original class information into the relevant name, address etc. \n",
    "    for each_review in range(loopcount):\n",
    "        \n",
    "        name_rest = [str(name).split(\">\", 1)[1].split(\"<\")[0]]*loopcount\n",
    "        \n",
    "        address_rest = [str(address).split(\">\",2)[2].split(\"<\",1)[0] + \", \" \\\n",
    "        + str(address).split(\">\", 4)[4].split(\"<\")[0] + str(address).split(\">\", 6)[6].split(\"<\", 1)[0]]*loopcount\n",
    "        \n",
    "        date_review = str(date[each_review]).split('\"', 4)[3]\n",
    "        dates.append(date_review) \n",
    "\n",
    "        headline_review = str(headline[each_review]).split(\">\", 1)[1].split(\"<\")[0]\n",
    "        headlines.append(headline_review)\n",
    "        \n",
    "        text_review = str(text[each_review]).split(\">\", 1)[1].split(\"<\")[0]\n",
    "        reviews.append(text_review)\n",
    "        \n",
    "        #Below we deal with the thousands seperator of'total reviews'. Since we want to express total number of reviews as an integer, we remove the comma. \n",
    "        total_reviews = str(total).replace(',', '')\n",
    "        total_reviews = int(total_reviews.split('(')[1].split(\")\")[0])\n",
    "        \n",
    "        # The variable 'last_page' shows the number of the comments on the last page. \n",
    "        last_page = int(str(link_last).split(\"'\")[7]) # number of the last page   \n",
    "        \n",
    "        #We then make 'remaining_reviews' a global variable in order to be able to call it in a function in the next section. \n",
    "        global remaining_reviews\n",
    "        remaining_reviews = total_reviews - (last_page - 1)*5\n",
    "        \n",
    "    # we create the dataframe with everything but the total number of reviews, for which we create a dictionary    \n",
    "    df = pd.DataFrame({\"name\":name_rest, \"address\":address_rest ,\"date_of_review\":dates, \"headlines\":headlines, \"reviews\":reviews})\n",
    "    total_review_dict = {name_rest[0]: total_reviews}\n",
    "    \n",
    "    return [df, total_review_dict]\n",
    "\n",
    "\n",
    "def information_lastpage(url, last_reviews):\n",
    "    \"\"\" This function scrapes the last page for each restuarant. The additional function is necessary as the last review page can have less than five comments\"\"\"\n",
    "\n",
    "    response = requests.get(url)\n",
    "    soup = BeautifulSoup(response.text, \"lxml\")\n",
    "\n",
    "    name = soup.find_all(attrs ={\"class\": \"HEADING\"})\n",
    "    address = soup.find_all(attrs ={\"class\": \"format_address\"})\n",
    "    total = soup.find_all(attrs = {\"class\": \"reviews_header_count block_title\"})\n",
    "    date = soup.find_all(attrs = {\"class\":\"ratingDate relativeDate\"})\n",
    "    headline = soup.find_all(attrs={\"class\":\"noQuotes\"})\n",
    "    text = soup.find_all(attrs = {\"class\": \"partial_entry\"})\n",
    "    link_last = soup.find_all(attrs = {\"class\":\"pageNum last taLnk \"})\n",
    "\n",
    "    dates = []\n",
    "    headlines = []\n",
    "    reviews = []\n",
    "\n",
    "   \n",
    "    for each_review in range(last_reviews):\n",
    "        \n",
    "        name_rest = [str(name).split(\">\", 1)[1].split(\"<\")[0]]*last\n",
    "\n",
    "        address_rest = [str(address).split(\">\",2)[2].split(\"<\",1)[0] + \", \" \\\n",
    "        + str(address).split(\">\", 4)[4].split(\"<\")[0] + str(address).split(\">\", 6)[6].split(\"<\", 1)[0]]*last\n",
    "    \n",
    "        date_review = str(date[each_review]).split('\"', 4)[3]\n",
    "        dates.append(date_review) \n",
    "\n",
    "        headline_review = str(headline[each_review]).split(\">\", 1)[1].split(\"<\")[0]\n",
    "        headlines.append(headline_review)\n",
    "        \n",
    "        text_review = str(text[each_review]).split(\">\", 1)[1].split(\"<\")[0]\n",
    "        reviews.append(text_review)      \n",
    "            \n",
    "    df = pd.DataFrame({\"name\":name_rest, \"address\":address_rest ,\"date_of_review\":dates, \"headlines\":headlines, \"reviews\":reviews})   \n",
    "    \n",
    "    return df\n",
    "\n"
   ]
